<?xml version="1.0"?>
<workflow xmlns="http://www.ecrion.com/eos/4.0">
	<flow-map></flow-map>
	<job-name-pattern/>
	<parameters>
		<parameter name="InvoiceNumber" value=""/>
	</parameters>
	<propagatable-tags/>
	<description/>
	<transient-run>false</transient-run>
	<start>
		<step>1</step>
	</start>
	<steps>
		<automatic id="1" title="Receive Invoice">
			<description>Get invoice data</description>
			<script>MHC.Plugins.GetDataPlugin</script>
			<parameters>
				<parameter name="datasource" value="./Invoice.edx"/>
			</parameters>
			<outcomes>
				<outcome name="Succeeded">
					<next>
						<step>2</step>
					</next>
				</outcome>
			</outcomes>
		</automatic>
		<automatic id="2" title="Invoice Review and Approve">
			<description>Invoice review and approval workflow</description>
			<script>MHC.Plugins.TaskPlugin</script>
			<parameters>
				<parameter name="description" value="Invoice review and approval workflow"/>
				<parameter name="TaskDefinitionId" value="{Invoice Review and Approve}"/>
				<parameter name="TaskIsAssignedTemplate" value="./Task is assigned.epr"/>
				<parameter name="ReminderTemplate" value="./Reminder.epr"/>
				<parameter name="EscalationTemplate" value="./Escalation.epr"/>
				<parameter name="EscalationTo" value=""/>
				<parameter name="RequestMoreInfoTemplate" value="./Request more information.epr"/>
				<parameter name="documentId" value="{XPATH:/root/invoice/id}"/>
				<parameter name="dueDate" value="{XPATH:/root/invoice/dueDate}"/>
			</parameters>
			<outcomes>
				<outcome name="Succeeded">
					<next>
						<step>-1</step>
					</next>
				</outcome>
			</outcomes>
		</automatic>
	</steps>
</workflow>