<?xml version="1.0"?>
<workflow xmlns="http://www.ecrion.com/eos/4.0">
  <flow-map></flow-map>
  <job-name-pattern></job-name-pattern>
  <parameters></parameters>
  <propagatable-tags></propagatable-tags>
  <description></description>
  <transient-run>false</transient-run>
  <start>
    <step>1</step>
  </start>
  <steps>
    <automatic id="1" title="Get Data">
      <description>Get New/Modified POs from ERP</description>
      <script>MHC.Plugins.GetDataPlugin</script>
      <parameters>
        <parameter name="datasource" value="./PurchaseOrder.edx"/>
        <parameter name="description" value="Get New/Modified POs from ERP"/>
        <parameter name="CONNECTION:BuiltIn REST API" value="WorkflowDefaultRestApi"/>
      </parameters>
      <outcomes>
        <outcome name="Succeeded">
          <next>
            <step>2</step>
          </next>
        </outcome>
      </outcomes>
    </automatic>

    <automatic id="2" title="REST API">
      <description>Retrieve Purchase Orders from ERP,  persist in Northstar</description>
      <script>MHC.Plugins.RestAPIPlugin</script>
      <parameters>
        <parameter name="connection-id" value="WorkflowDefaultRestApi"/>
        <parameter name="Address" value="api/internal/purchase-orders"/>
        <parameter name="Method" value="POST"/>
        <parameter name="BodyType" value="raw"/>
        <parameter name="ContentType" value="JSON"/>
        <parameter name="BodyContentText" value="{  &quot;filePath&quot;: &quot;{InputFilePath}&quot; }"/>
        <parameter name="description" value="Retrieve Purchase Orders from ERP,  persist in Northstar"/>
      </parameters>
      <outcomes>
        <outcome name="Succeeded">
          <next>
            <step>-1</step>
          </next>
        </outcome>
      </outcomes>
    </automatic>
  </steps>
</workflow>
