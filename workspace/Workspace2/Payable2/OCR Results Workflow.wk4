<?xml version="1.0"?>
<workflow xmlns="http://www.ecrion.com/eos/4.0">
  <flow-map></flow-map>
  <job-name-pattern></job-name-pattern>
  <parameters></parameters>
  <propagatable-tags></propagatable-tags>
  <description></description>
  <transient-run>false</transient-run>
  <start>
    <step>1</step>
  </start>
  <steps>
    <automatic id="1" title="SFTP">

      <script>MHC.Plugins.SftpPlugin</script>
      <parameters>
        <parameter name="RequestType" value="Get Directories"/>
        <parameter name="type" value="SFTP"/>
      </parameters>
      <outcomes>
        <outcome name="Succeeded">
          <next>
            <step>2</step>
          </next>
        </outcome>
      </outcomes>
    </automatic>

    <automatic id="2" title="Group">

      <script>MHC.Plugins.GroupPlugin</script>
      <parameters>
        <parameter name="GroupKey" value="{InputFilePath}"/>
      </parameters>
      <outcomes>
        <outcome name="Succeeded">
          <next>
            <step>3</step>
          </next>
        </outcome>
      </outcomes>
    </automatic>

    <automatic id="3" title="REST API">

      <script>MHC.Plugins.RestAPIPlugin</script>
      <parameters>
        <parameter name="connection-id" value="WorkflowDefaultRestApi"/>
        <parameter name="Method" value="POST"/>
        <parameter name="Address" value="api/internal/ocr/process-results"/>
        <parameter name="BodyType" value="raw"/>
        <parameter name="ContentType" value="JSON"/>
        <parameter name="BodyContentText" value="{
&quot;TenantId&quot;: &quot;{TenantId}&quot;,
&quot;RealmName&quot;: &quot;{Tenant}&quot;,
&quot;ProjectId&quot;: &quot;{ProjectId}&quot;,
&quot;OcrResultsDirectory&quot;: &quot;{InputFilePath}&quot; 
}"/>
      </parameters>
      <outcomes>
        <outcome name="Succeeded">
          <next>
            <step>-1</step>
          </next>
        </outcome>
      </outcomes>
    </automatic>
  </steps>
</workflow>